    <script>
        var LED = '<%=results[0].LED%>';
        var PUMP = '<%=results[0].PUMP%>';
        var INDI = '<%=results[0].INDI%>';
        var RESPONSE = '<%=results[0].RESPONSE%>';
        var ETC = '<%=results[0].ETC%>';
        // Set new default font family and font color to mimic Bootstrap's default styling
        Chart.defaults.global.defaultFontFamily = '-apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif';
        Chart.defaults.global.defaultFontColor = '#292b2c';

        // Area Chart Example
        var ctx = document.getElementById("myAreaChart");
        var myLineChart = new Chart(ctx, {
            type: 'line',
            data: {
                labels: ["LED Error", "Water Pump Error", "Indicator Error", "Response Error", "ETC"],
                datasets: [{
                    label: "횟수",
                    lineTension: 0.3,
                    backgroundColor: "rgba(161,38,124,0.2)",//칠해지는 부분
                    borderColor: "rgba(161,38,124,1)",//선색깔
                    pointRadius: 5,
                    pointBackgroundColor: "rgba(161,38,124,1)",//점색깔
                    pointBorderColor: "rgba(255,255,255,0.8)",
                    pointHoverRadius: 5,
                    pointHoverBackgroundColor: "rgba(161,38,124,1)",//올렸을때
                    pointHitRadius: 50,
                    pointBorderWidth: 2,
                    data: [LED, PUMP, INDI, RESPONSE, ETC],
                }],
            },
            options: {
                scales: {
                    xAxes: [{
                        time: {
                            unit: 'date'
                        },
                        gridLines: {
                            display: false
                        },
                        ticks: {
                            fontColor: 'white',
                            fontWeight: 'bold',
                        }
                    }],
                    yAxes: [{
                        ticks: {
                            fontColor: 'white',
                            min: 0,
                            max: 1000,
                            maxTicksLimit: 5
                        },
                        gridLines: {
                            color: "rgba(0, 0, 0, .125)",//가이드 라인 색
                        }
                    }],
                },
                legend: {
                    display: false
                }
            }
        });

    </script>
